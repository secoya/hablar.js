{
    "compilerOptions": {
        "strictNullChecks": true,
        "module": "commonjs",
        "target": "es5",
        "declaration": false,
        "noEmit": false,
        "newLine": "LF",
        "outDir": "lib",
        "noImplicitAny": true,
        "allowJs": false,
        "moduleResolution": "node",
        "lib": [
            "es6",
            "es2015"
        ]
    },
    "include": [
        "typings/**/*.d.ts",
        "src/**/*.ts",
        "test/**/*.ts"
    ]
}